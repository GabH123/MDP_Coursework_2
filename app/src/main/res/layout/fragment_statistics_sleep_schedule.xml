<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    tools:context=".View.StatisticsFragment.StatisticsSleepScheduleFragment" >

    <TextView
        android:id="@+id/textView10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Current Sleeping Schedule"
        android:textSize="32dp" />

    <TextView
        android:id="@+id/currentSleepScheduleTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="-:- - -:-"
        android:textSize="28dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="New Sleepinig Schedule"
        android:textSize="32dp" />

    <TextView
        android:id="@+id/newSleepScheduleTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="-:- - -:-"
        android:textSize="28dp" />

</LinearLayout>